{
  "manifest_version": 2,

  "name": "UofT Prerequisite Checker",
  "version": "0.2.0",
  "description": "Enhances the UofT course calendar",

  "content_scripts": [{
    "js": ["jquery-3.3.1.min.js", "content.js"],
    "matches": ["https://fas.calendar.utoronto.ca/course/*"]
  }],
  "permissions": [
    "*://cobalt.qas.im/*"
  ],
  "web_accessible_resources": [
    "evals.json"
  ]

}
